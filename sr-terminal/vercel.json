{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Embedder-Policy", "value": "credentialless" },
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
      ]
    }
  ],
  "redirects": [
    { "source": "/", "destination": "https://sreekarreddy.com/projects/terminal", "permanent": true }
  ]
}
